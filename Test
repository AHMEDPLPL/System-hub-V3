local function loadscript()

local OrionLib = loadstring(game:HttpGet(("https://raw.githubusercontent.com/AHMEDPLPL/System-hub-V3/refs/heads/main/Themes/Black%20Theme")))()
local st = tick()

local Window = OrionLib:MakeWindow({
    Name = "System Hub 3.0.0",
    IntroText = "loading..."
})

local Tab = Window:MakeTab({
    Name = "Main",
    Icon = "rbxassetid://6026568227",
    PremiumOnly = false
})

Tab:AddParagraph(
    "Welcome, " .. game.Players.LocalPlayer.DisplayName,
    "Hello @" .. game.Players.LocalPlayer.Name .. 
    "! We're glad to have you here. Feel free to explore all the features available in System Hub V3. If you have any new ideas that you’d like to add to the hub, send us feedback, and we’ll consider adding it, or just DM me ahmed121212 in discord. Enjoy your time!"
)

Tab:AddParagraph(
    "Scripts issues",
    "If you notice scripts that are not working or the hub not working properly in a game, or if the hub is not working with your executor, please provide feedback so we can fix the issue as soon as possible."
)

local HttpService = game:GetService("HttpService")
local player = game.Players.LocalPlayer
local feedbackTimestamps = {}
local placeName = game:GetService("MarketplaceService"):GetProductInfo(game.PlaceId).Name
local request = syn and syn.request or http and http.request or http_request or 
                fluxus and fluxus.request or request or oxygen and oxygen.http or 
                comet and comet.request or yoink and yoink.request or venus and venus.request or 
                WRD and WRD.request or solara and solara.request or HttpPost
        

local function cleanupOldTimestamps()
    local currentTime = tick()
    for i = #feedbackTimestamps, 1, -1 do
        if currentTime - feedbackTimestamps[i] > 1200 then
            table.remove(feedbackTimestamps, i)
        end
    end
end

local function sendFeedback(txt)
    cleanupOldTimestamps()
    if #feedbackTimestamps >= 3 then
        OrionLib:MakeNotification({
            Name = "System Hub",
            Content = "You can only send 3 feedbacks every 20 minutes.",
            Image = "rbxassetid://6031075938",
            Time = 5
        })
        return
    end

    local webhook = "https://discord.com/api/webhooks/1276593397662089337/i-Y7kwaK9OxgH9OFosXclajJhTCzDfB_oHc3Zo2PJ7RZCFYLhGjLQ7t0H7z823bougZv"
    local data = {
        ["content"] = "",
        ["embeds"] = {
            {
                ["title"] = "Feedback From : " .. player.Name,
                ["description"] = "Executor : "..(identifyexecutor and identifyexecutor() or "Unknown").."   Game : "..placeName.."           FeedBack : "..txt
            }
        }
    }

    local jsonData = HttpService:JSONEncode(data)
    local headers = {
        ["Content-Type"] = "application/json"
    }

    if request then
        request({
            Url = webhook,
            Method = "POST",
            Headers = headers,
            Body = jsonData
        })
        table.insert(feedbackTimestamps, tick())
        OrionLib:MakeNotification({
            Name = "System Hub",
            Content = "Feedback sent successfully",
            Image = "rbxassetid://6031075938",
            Time = 5
        })
    else
        OrionLib:MakeNotification({
            Name = "System Hub",
            Content = "Failed to send feedback",
            Image = "rbxassetid://6031075938",
            Time = 5
        })
    end
end

local userInput = ""
Tab:AddTextbox({
    Name = "Feedback",
    Default = "",
    TextDisappear = true,
    Callback = function(txt)
        userInput = txt
    end
})

Tab:AddButton({
    Name = "Send",
    Callback = function()
        if userInput ~= "" then
            sendFeedback(userInput)
        else
            OrionLib:MakeNotification({
                Name = "System Hub",
                Content = "No Feedback To send",
                Image = "rbxassetid://6031075938",
                Time = 5
            })
        end
    end
})

Tab:AddButton({
   Name = "Discord Server",
   Callback = function()
       setclipboard("https://discord.gg/hh9gBDaWd4")
           OrionLib:MakeNotification({
                Name = "Copied Link",
                Content = "Discord Server Link copied to clipboard!",
                Image = "rbxassetid://6031075938",
                Time = 5
           })
      end
})

Tab:AddLabel("                                           Update Logs")
Tab:AddLabel("Released First Version")




local function loadkeysystem()

local a=2^32;local b=a-1;local function c(d,e)local f,g=0,1;while d~=0 or e~=0 do local h,i=d%2,e%2;local j=(h+i)%2;f=f+j*g;d=math.floor(d/2)e=math.floor(e/2)g=g*2 end;return f%a end;local function k(d,e,l,...)local m;if e then d=d%a;e=e%a;m=c(d,e)if l then m=k(m,l,...)end;return m elseif d then return d%a else return 0 end end;local function n(d,e,l,...)local m;if e then d=d%a;e=e%a;m=(d+e-c(d,e))/2;if l then m=n(m,l,...)end;return m elseif d then return d%a else return b end end;local function o(p)return b-p end;local function q(d,r)if r<0 then return lshift(d,-r)end;return math.floor(d%2^32/2^r)end;local function s(p,r)if r>31 or r<-31 then return 0 end;return q(p%a,r)end;local function lshift(d,r)if r<0 then return s(d,-r)end;return d*2^r%2^32 end;local function t(p,r)p=p%a;r=r%32;local u=n(p,2^r-1)return s(p,r)+lshift(u,32-r)end;local v={0x428a2f98,0x71374491,0xb5c0fbcf,0xe9b5dba5,0x3956c25b,0x59f111f1,0x923f82a4,0xab1c5ed5,0xd807aa98,0x12835b01,0x243185be,0x550c7dc3,0x72be5d74,0x80deb1fe,0x9bdc06a7,0xc19bf174,0xe49b69c1,0xefbe4786,0x0fc19dc6,0x240ca1cc,0x2de92c6f,0x4a7484aa,0x5cb0a9dc,0x76f988da,0x983e5152,0xa831c66d,0xb00327c8,0xbf597fc7,0xc6e00bf3,0xd5a79147,0x06ca6351,0x14292967,0x27b70a85,0x2e1b2138,0x4d2c6dfc,0x53380d13,0x650a7354,0x766a0abb,0x81c2c92e,0x92722c85,0xa2bfe8a1,0xa81a664b,0xc24b8b70,0xc76c51a3,0xd192e819,0xd6990624,0xf40e3585,0x106aa070,0x19a4c116,0x1e376c08,0x2748774c,0x34b0bcb5,0x391c0cb3,0x4ed8aa4a,0x5b9cca4f,0x682e6ff3,0x748f82ee,0x78a5636f,0x84c87814,0x8cc70208,0x90befffa,0xa4506ceb,0xbef9a3f7,0xc67178f2}local function w(x)return string.gsub(x,".",function(l)return string.format("%02x",string.byte(l))end)end;local function y(z,A)local x=""for B=1,A do local C=z%256;x=string.char(C)..x;z=(z-C)/256 end;return x end;local function D(x,B)local A=0;for B=B,B+3 do A=A*256+string.byte(x,B)end;return A end;local function E(F,G)local H=64-(G+9)%64;G=y(8*G,8)F=F.."\128"..string.rep("\0",H)..G;assert(#F%64==0)return F end;local function I(J)J[1]=0x