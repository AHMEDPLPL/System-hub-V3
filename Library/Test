if System_Hub3 == true then
    warn("System hub Already Loaded")
    return
end

pcall(function() getgenv().System_Hub3 = true end)


local Fluent = loadstring(game:HttpGet("https://raw.githubusercontent.com/AHMEDPLPL/System-hub-V3/refs/heads/main/Library/Library"))()
local SaveManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/AHMEDPLPL/System-hub-V3/refs/heads/main/Library/Save%20Manager"))()
local InterfaceManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/AHMEDPLPL/System-hub-V3/refs/heads/main/Library/Interface%20Manager"))()

local Window = Fluent:CreateWindow({
    Title = "System Hub 3.0.0",
    SubTitle = "by Ahmed",
    TabWidth = 160,
    Size = UDim2.fromOffset(560,360),
    Acrylic = false,
    Theme = "Dark",
    MinimizeKey = Enum.KeyCode.RightShift
})

UserInputService = game:GetService("UserInputService")
local IsOnMobile = table.find({
Enum.Platform.IOS,
Enum.Platform.Android
}, UserInputService:GetPlatform())
local Sky = {}
if IsOnMobile then
local ScreenGui = Instance.new("ScreenGui")
ScreenGui.Name = "SystemHubGui"
ScreenGui.Parent = gethui and gethui() or game.CoreGui
ScreenGui.ResetOnSpawn = false
ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

local Toggle = Instance.new("ImageButton")
Toggle.Name = "Toggle"
Toggle.Parent = ScreenGui
Toggle.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
Toggle.Position = UDim2.new(0, 170, 0, 40)
Toggle.Size = UDim2.new(0, 60, 0, 60)
Toggle.Image = "rbxassetid://74132088634336"
Toggle.BackgroundTransparency = 0

Toggle.Draggable = true
Toggle.Visible = true

local Corner1 = Instance.new("UICorner")
Corner1.CornerRadius = UDim.new(0, 3)
Corner1.Parent = Toggle

Toggle.MouseButton1Click:Connect(function()
  for _, gui in pairs(game:GetService("CoreGui"):GetDescendants()) do
	if gui.Name == "SystemHubV3Gui" then
		for _, obj in pairs(gui:GetDescendants()) do
			if obj:IsA("Frame") and obj.Name == "SystemHubV3MainFrame" then
				obj.Visible = not obj.Visible
			end
		end
	end
end
end)
else
Fluent:Notify{Title = "Interface", Content = "Press RightShift to toggle the inteface.", Duration = 6}
end



--Fluent provides Lucide Icons https://lucide.dev/icons/ for the tabs, icons are optional
local Tabs = {
    Main = Window:AddTab({ Title = "Main", Icon = "shield" }),
    Settings = Window:AddTab({ Title = "Settings", Icon = "settings" })
}

local Options = Fluent.Options
